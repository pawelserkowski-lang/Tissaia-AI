
{
  "name": "tissaia-architect-engine",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "backend": "tsx --watch backend/server.ts",
    "backend:prod": "tsx backend/server.ts",
    "dev:all": "concurrently \"npm run backend\" \"npm run dev\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "docs": "jsdoc -c jsdoc.json",
    "electron:dev": "electron electron/main.js",
    "electron:build": "npm run build && electron-builder",
    "electron:build:mac": "npm run build && electron-builder --mac",
    "electron:build:win": "npm run build && electron-builder --win",
    "electron:build:linux": "npm run build && electron-builder --linux",
    "dev:all:electron": "concurrently \"npm run backend\" \"npm run dev\" \"wait-on http://localhost:5173 && npm run electron:dev\""
  },
  "dependencies": {
    "@google/genai": "^1.31.0",
    "@vercel/analytics": "^1.3.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jszip": "^3.10.1",
    "multer": "^1.4.5-lts.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^1.0.4",
    "concurrently": "^9.1.0",
    "jsdom": "^23.0.1",
    "jsdoc": "^4.0.2",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.0.4"
  }
}
